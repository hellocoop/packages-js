{
    "name": "@hellocoop/fastify",
    "version": "1.14.6",
    "description": "Fastify SDK for HellÅ https://hello.dev",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hellocoop/packages.git"
    },
    "homepage": "https://www.hello.dev/docs/sdks/fastify",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js"
    },
    "files": [
        "dist/"
    ],
    "keywords": [
        "fastify",
        "nodejs",
        "node.js",
        "hello",
        "openid",
        "oidc",
        "sso"
    ],
    "author": {
        "name": "Hello Identity Co-op",
